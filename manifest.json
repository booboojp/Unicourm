{
    "manifest_version": 3,
    "name": "Unicourm",
    "version": "1.0.0",
    "description": "Booboos Custom Browser Startup Page!",
    "author": "Booboo",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png",
      "192": "icons/icon192.png"
    },
    "web_accessible_resources": [{
      "resources": [
        "background.gif",
        "background-music.mp3"
      ],
      "matches": ["<all_urls>"]
    }],
    "homepage_url": "https://booboojp.github.io/browser-startup-page/"
}